@model IEnumerable<Mittagessen.Data.Entities.Lunch>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Sprava/Views/Shared/_Layout.cshtml";
}

 
@Html.Action("Create")

<table class="table">
@foreach(var lunch in Model)
{
    <tr>
        <td>@Html.ActionLink(lunch.LunchDate.ToString(),  "Edit", new { id=lunch.Id })</td>
        <td>@lunch.CookedMeal.Name</td>
        <td>@lunch.NumberOfPortions</td>
    </tr>
}
</table>

